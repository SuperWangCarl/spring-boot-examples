<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.carlme.mybatisplus.persistence.mapper.CommonNoticeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.carlme.multi.mybatisplus.persistence.po.CommonNotice">
        <id column="id" property="id"/>
        <result column="notice_name" property="noticeName"/>
        <result column="create_id" property="createId"/>
        <result column="modified_id" property="modifiedId"/>
        <result column="create_time" property="createTime"/>
        <result column="modified_time" property="modifiedTime"/>
        <result column="comment" property="comment"/>
        <result column="del_flag" property="delFlag"/>
        <result column="use_flag" property="useFlag"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, notice_name, create_id, modified_id, create_time, modified_time, comment, del_flag, use_flag
    </sql>

</mapper>
